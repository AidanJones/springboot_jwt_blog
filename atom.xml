---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
	<title>qraftlabs blog</title>
	<link href="http://blog.qraftlabs.com/atom.xml" rel="self" type="application/atom+xml" />
	<link href="http://blog.qraftlabs.com/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://blog.qraftlabs.com/</id>
	<author>
		<name>qraftlabs</name>
		<email>admin@qraftlabs.com</email>
	</author>

	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link type="text/html" rel="alternate" href="http://blog.qraftlabs.com{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://blog.qraftlabs.com{{ post.id }}/</id>
		<author>
			<name>{{ post.author.name }}</name>
			<uri>{{ post.author.url }}</uri>
		</author>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
 
</feed>